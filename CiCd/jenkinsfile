pipeline{
    agent any
    stages{
        stage('checkout') {
            steps {
                cleanWs()
                checkout scm
            }
        }
        stage("build"){
            steps{
                sh 'dotnet build --configuration Release'
            }
        }
        stage("test"){
            steps{
                sh 'dotnet test --no-build' --collect:"XPlat Code Coverage"
            }
        }
    }    
}